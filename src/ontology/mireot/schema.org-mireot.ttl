    schema:domainIncludes schema:Organization ;
    schema:rangeIncludes xsd:string ;
    rdfs:comment "The official name of the organization, e.g. the registered company name." .

schema:maxValue a owl:DatatypeProperty ;
    rdfs:label "maxValue" ;
